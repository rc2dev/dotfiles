[filter "borg-mount"]
	clean = "sed \
		-e 's/default_repos_dir=.*$/default_repos_dir=/' \
		-e 's/default_passcommand=.*$/default_passcommand=/'"

[filter "flameshot"]
  clean = "sed -E '/^savePath/d; /^draw/d'"

[filter "xsettingsd"]
	clean = "sed -E 's#^Net/ThemeName .*$#Net/ThemeName \"\"#;s#^Net/IconThemeName .*$#Net/IconThemeName \"\"#'"
