# "Guest profile" calls chromium's profile selector
super + {_, ctrl, shift} + a
  chromium {--profile-directory="Default", --profile-directory="Guest Profile", --temp-profile}

super + c
  dmenu_yt

super + alt + d
  light-dark

super + e
  emacs

super + f
  $TERMINAL -e ranger

super + shift + f
  nemo

super + m
  $TERMINAL -n cmus -e cmus

super + n
  $TERMINAL -e notes name

super + shift + n
  dmenu_notes

super + o
  $TERMINAL -e notes journal

super + p
  keepassxc

super + shift + q
  xkill

super + r
  dmenu_url

super + x
  $TERMINAL -e zsh -ic 'o'

super + {_, shift} + w
  x-www-browser {_, --incognito}

ctrl + alt + v
  clipmenu

XF86Calculator
  gnome-calculator

Print
  flameshot gui

Print + shift
  flameshot launcher

