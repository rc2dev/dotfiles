# "Guest profile" calls chromium's profile selector
super + {_, ctrl, shift} + a
  chromium {--profile-directory="Default", --profile-directory="Guest Profile", --temp-profile}

super + alt + d
  light-dark

super + e
  emacs

super + f
  $TERMINAL -e ranger

super + shift + f
  nemo

super + {_, shift, alt} + n
  {$TERMINAL -e notes name, dmenu_notes, $TERMINAL -e notes journal}

super + p
  keepassxc

super + shift + q
  xkill

super + r
  dmenu_url

# Try to copy link from browser before running dmenu_url.
# Both tilde and --clear-modifiers are a workaround for this to work.
~super + shift + r
  xdotool key --clearmodifiers ctrl+l && xdotool key --clearmodifiers ctrl+c && dmenu_url

super + s; {c, e, l, m, t, v, w, x, y}
 { \
  localc, \
  thunderbird, \
  thingy, \
  $TERMINAL -n cmus -e cmus, \
  org.telegram.desktop, \
  virt-manager, \
  io.bit3.WhatsAppQT, \
  $TERMINAL -e zsh -ic 'o', \
  dmenu_yt, \
 }

super + {_, shift} + w
  x-www-browser {_, --incognito}

ctrl + alt + v
  clipmenu

XF86Calculator
  gnome-calculator

Print
  flameshot gui

Print + shift
  flameshot launcher

