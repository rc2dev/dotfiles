super + a
  dmenu_radio

super + b
  dmenu-bluetooth

super + c
  gnome-calculator

super + shift + d
  light-dark

super + {_, shift} + e
  { \
    $TERMINAL -e nvim +History, \
    $TERMINAL -e yadm enter nvim +GFiles \
  }

super + f
  $TERMINAL -e ranger

super + shift + f
  nautilus

super + {_, shift} + n
  $TERMINAL -e notes { name, dump }

super + p
  keepassxc

super + shift + q
  xkill

super + r
  dmenu_url

super + s; {c, e, m, v, w}
 { \
  chromium-browser --temp-profile, \
  thunderbird, \
  $TERMINAL -n cmus -e cmus, \
  pavucontrol, \
  qimgv $WALLPAPERS, \
 }

super + x
  $TERMINAL -e bash -c "\
    fd --follow --exclude 'Cursos/' --exclude 'Notes/' . ~/Documents ~/Downloads $NOTES/resources ~/Videos | \
    fzf --height 100% --margin 7%\,5% --no-preview | \
    xargs -r -d'\n' -P 0 -n 1 xdg-open \
  "

super + {_, shift} + w
  $BROWSER {_, --incognito}

super + y
  dmenu_yt

super + z
  $BROWSER https://www.myfitnesspal.com/pt/food/diary

super + {_, shift, ctrl } + period
  dunstctl { context, close, history-pop }

super + Return
  $TERMINAL

alt + Tab
  dswitcher

ctrl + alt + v
  clipmenu

Print
  flameshot gui

Print + shift
  flameshot launcher

XF86Calculator
  gnome-calculator

