super + a
  dmenu_radio

super + b
  blueman-manager

super + c
  gnome-calculator

super + shift + d
  light-dark

super + e; {t, y}
  { \
    $TERMINAL -e nvim +History, \
    $TERMINAL -e dotfiles cmd nvim +GFiles \
  }

super + f
  $TERMINAL -e ranger

super + shift + f
  nautilus

super + {_, shift, ctrl} + n
  { \
    $TERMINAL -e notes name, \
    $TERMINAL -e notes dump, \
    $TERMINAL -n plan -e notes today, \
  }

super + p
  keepassxc

super + shift + q
  xkill

super + r
  dmenu_url

super + s; {m, w}
 { \
  $TERMINAL -n cmus -e cmus, \
  qimgv $WALLPAPERS, \
 }

super + x
  $TERMINAL -e bash -c "\
    fd --follow --exclude 'Cursos/' --exclude 'Notes/' . ~/Documents ~/Downloads $NOTES/resources ~/Videos | \
    fzf --height 100% --margin 7%\,5% --no-preview | \
    xargs -r -d'\n' -P 0 -n 1 xdg-open \
  "

super + {_, shift} + w
  $BROWSER {_, --incognito}

super + y
  dmenu_yt

super + {_, shift} + z
  { \
    $BROWSER https://www.myfitnesspal.com/pt/food/diary, \
    xdg-open ~/Documents/Em.ods, \
  }

super + {_, shift, ctrl } + period
  dunstctl { context, close, history-pop }

super + Return
  $TERMINAL

alt + Tab
  dswitcher

ctrl + alt + v
  clipmenu

Print
  flameshot gui

Print + shift
  flameshot launcher

XF86Calculator
  gnome-calculator

