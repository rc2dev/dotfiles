super + shift + d
  light-dark

super + b
  dmenu-bluetooth

super + e
  $TERMINAL -e vim +History

super + f
  $TERMINAL -e ranger

super + shift + f
  nemo

super + {_, shift, ctrl} + n
  { \
    $TERMINAL -e notes name, \
    $TERMINAL -e notes dump, \
    $TERMINAL -n plan -e notes plan \
  }

super + p
  keepassxc

super + shift + q
  xkill

super + r
  dmenu_url

# Try to copy link from browser before running dmenu_url.
# Both tilde and --clear-modifiers are a workaround for this to work.
~super + shift + r
  xdotool key --clearmodifiers ctrl+l && xdotool key --clearmodifiers ctrl+c && dmenu_url

super + s; {c, C, m, r, w, x, y}
 { \
  chromium, \
  chromium --temp-profile, \
  $TERMINAL -n cmus -e cmus, \
  de.haeckerfelix.Shortwave, \
  qimgv $WALLPAPERS, \
  $TERMINAL -e zsh -c "fd $FD_ARGS --no-hidden --exclude 'Cursos/' --exclude 'Notes/' . ~/Documents ~/Downloads $NOTES/resources | fzf --height 100% --margin 7%\,5% | xargs -r -d'\n' -P 0 -n 1 xdg-open", \
  dmenu_yt, \
 }

super + {_, shift} + w
  x-www-browser {_, --incognito}

super + {_, shift, ctrl, alt } + period
  dunstctl { action 0, close, history-pop, context }

super + Return
  $TERMINAL

ctrl + alt + v
  clipmenu

XF86Calculator
  gnome-calculator

Print
  flameshot gui

Print + shift
  flameshot launcher

